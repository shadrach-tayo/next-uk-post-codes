{
    "family": "next-uk-post-codes",
    "executionRoleArn": "arn:aws:iam::751808579985:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "next-uk-post-codes",
            "image": "751808579985.dkr.ecr.us-east-1.amazonaws.com/next-uk-post-codes:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}